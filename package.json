{
  "name": "bootstrap-11ty-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all clear --parallel watch-html watch-css",
    "watch-html": "eleventy --serve",
    "watch-css": "tailwindcss -i src/main.css -o _site/main.css --watch",
    "build": "npm-run-all clear build-html build-css build-svg print-decks",
    "build-html": "eleventy",
    "build-css": "tailwindcss -i src/main.css -o _site/main.css --minify",
    "build-svg": "purge-svg-sprite -c \"src/**/*.njk\" -i _site/assets/images/bootstrap-icons.svg -o _site/assets/images/bootstrap-icons.svg",
    "print-decks": "node scripts/print-decks",
    "clear": "shx rm -rf _site/ src/dist/",
    "prepare": "husky install",
    "format": "prettier --write *.{js,html,css,scss,json}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaraqwahab/bootstrap-11ty-starter.git"
  },
  "bugs": {
    "url": "https://github.com/mubaraqwahab/bootstrap-11ty-starter/issues"
  },
  "homepage": "https://github.com/mubaraqwahab/bootstrap-11ty-starter#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.2",
    "bootstrap-icons": "^1.8.1",
    "codemirror": "^5.65.0",
    "github-slugger": "^1.4.0",
    "highlight.js": "^11.3.1",
    "html-minifier-terser": "^6.0.2",
    "jsdom": "^19.0.0",
    "kebab-case": "^1.0.1",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "plain-tag": "^0.1.3",
    "postcss": "^8.4.8",
    "puppeteer": "^13.5.2",
    "purge-svg-sprite": "^0.0.4",
    "reveal.js": "^4.3.0",
    "serve-handler": "^6.1.3",
    "shx": "^0.3.3",
    "tailwindcss": "^3.0.23",
    "terser": "^5.9.0"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "prettier": "2.3.2"
  }
}
